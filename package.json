{
  "name": "gridoo",
  "version": "1.0.2",
  "description":
    "Simple Virtualized Grid with paging and auto scrollable fixed to container, with detailed rows",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run webpack && rimraf ./dist/src",
    "dev": "npm run lint && npm run clean && npm run webpackwatch",
    "clean": "rimraf ./dist",
    "lint": "tslint -c tslint.json 'src/*.ts'",
    "webpack": "webpack --progress --colors --config webpack.config.js --output-path ./dist -p",
    "webpackwatch": "webpack --watch --progress --colors --config webpack.config.js --output-path ./dist -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohan3usturge/Gridoo.git"
  },
  "types": "index.d.ts",
  "typescript": {
    "definition": "index.d.ts"
  },
  "author": "rohan.usturge@hotmail.com (Rohan Usturge)",
  "license": "MIT",
  "devDependencies": {
    "@types/handlebars": "^4.0.36",
    "@types/jquery": "^3.2.16",
    "bower": "^1.8.2",
    "dts-bundle": "^0.7.3",
    "html-loader": "^0.5.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "handlebars": "^4.0.11",
    "jquery": "^3.2.1"
  }
}
